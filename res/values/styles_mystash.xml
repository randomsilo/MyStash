<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <style name="DialogMyStash" parent="@android:style/Theme.Dialog">
      <item name="android:layout_width">fill_parent</item>
      <item name="android:layout_height">wrap_content</item>
      
      <item name="android:windowFullscreen">false</item>
      <item name="android:windowIsFloating">false</item>
      
      <item name="android:textColor">@color/theme_lite</item>
      <item name="@android:background">@color/tinted</item>
  </style>
    
  <style name="AutoCompleteTextViewMyStash" parent="android:Widget.Holo.Light.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/mystash_list_selector_holo_light</item>
	    <item name="android:background">@drawable/mystash_edit_text_holo_light</item>
  </style>

  <style name="ButtonMyStash" parent="android:Widget.Holo.Light.Button">
	  <item name="android:background">@drawable/mystash_btn_default_holo_light</item>
  </style>

  <style name="ImageButtonMyStash" parent="android:Widget.Holo.Light.ImageButton">
	  <item name="android:background">@drawable/mystash_btn_default_holo_light</item>
  </style>

  <style name="SpinnerMyStash" parent="android:Widget.Holo.Light.Spinner">
      <item name="android:background">@drawable/mystash_spinner_background_holo_light</item>
      <item name="android:dropDownSelector">@drawable/mystash_list_selector_holo_light</item>
  </style>

  <style name="TabMyStash">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/mystash_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextMyStash">
	  <item name="android:textColor">#000000</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarMyStash" parent="android:Widget.Holo.Light.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/mystash_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/mystash_progress_indeterminate_horizontal_holo_light</item>
  </style>

  <style name="SeekBarMyStash" parent="android:Widget.Holo.Light.SeekBar">
      <item name="android:progressDrawable">@drawable/mystash_scrubber_progress_horizontal_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/mystash_scrubber_progress_horizontal_holo_light</item>
      <item name="android:thumb">@drawable/mystash_scrubber_control_selector_holo_light</item>
  </style>

  <style name="RatingBarMyStash" parent="android:Widget.Holo.Light.RatingBar">
      <item name="android:progressDrawable">@drawable/mystash_ratingbar_full_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/mystash_ratingbar_full_holo_light</item>
  </style>

  <style name="RatingBarBigMyStash" parent="android:Widget.Holo.Light.RatingBar.Indicator">
      <item name="android:progressDrawable">@drawable/mystash_ratingbar_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/mystash_ratingbar_holo_light</item>
  </style>

  <style name="RatingBarSmallMyStash" parent="android:Widget.Holo.Light.RatingBar.Small">
      <item name="android:progressDrawable">@drawable/mystash_ratingbar_small_holo_light</item>
      <item name="android:indeterminateDrawable">@drawable/mystash_ratingbar_small_holo_light</item>
  </style>

  <style name="ToggleMyStash" parent="android:Widget.Holo.Light.Button.Toggle">
      <item name="android:background">@drawable/mystash_btn_toggle_holo_light</item>
  </style>

</resources>